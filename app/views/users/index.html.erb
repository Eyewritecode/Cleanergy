<div class="jumbotron">
  <h1>Admin Dashboard</h1>
  <h2>List of WeBill customers! </h2>
</div>
<div class="container">
<div align="center">
<table class="table table-bordered">
  <thead> 
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>Sign in Count</th>
      <th>Last sign in</th>
      <th colspan= "2">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.sign_in_count %></td>
        <td><%= user.last_sign_in_at %></td>
        <td><a href="#"> Delete </a></td>
        <td><a href="#"> Edit </a></td>
      </tr>
    <%end%>
  </tbody>
</table>

<br>
</div>
<%= link_to "Back", root_path %>
</div>